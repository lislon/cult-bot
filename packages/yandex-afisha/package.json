{
  "name": "yandex-afisha",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Igor Golovin <lislon@mail.ru>",
  "license": "unlicensed",
  "private": true,
  "esm": true,
  "mode": "auto",
  "scripts": {
    "start": "dotenv ts-node src/server.ts --cache-directory dist/ts-node-dev-cache --transpile-only",
    "dev": "ts-node-dev -r source-map-support/register --respawn --ignore-watch node_modules src/server.ts --cache-directory dist/ts-node-dev-cache --transpile-only",
    "db:up": "cross-env TZ=Europe/Moscow db-migrate up --table=p_migrations",
    "db:down": "cross-env TZ=Europe/Moscow db-migrate down --table=p_migrations",
    "db:new": "cross-env TZ=Europe/Moscow db-migrate create --table=p_migrations",
    "parse:orig": "dotenv ts-node-dev -r source-map-support/register src/parser.ts -- --orig",
    "parse:snapshot": "dotenv ts-node-dev  -r source-map-support/register src/parser.ts -- --snapshot",
    "convert": "dotenv ts-node t/convert.ts",
    "build": "tsc --build && eslint .",
    "test": "tsc --help",
    "dev-no-restart": "tsc && nodemon --inspect"
  },
  "dependencies": {
    "@culthub/google-docs": "^1.0.0",
    "@culthub/universal-db-sync": "^1.0.0",
    "@culthub/pg-utils": "^1.0.0",
    "crawler": "^1.3.0",
    "date-fns": "^2.16.1",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "glob-promise": "^3.4.0",
    "googleapis": "^67.0.0",
    "jsdom": "^16.4.0",
    "typescript": "^4.1.5",
    "source-map-support": "^0.5.19",
    "dotenv": "^8.2.0",
    "pg": "^8.4.2",
    "pg-promise": "^10.6.1"
  },
  "devDependencies": {
    "@types/crawler": "^1.2.0",
    "@types/express": "^4.17.11",
    "@types/jsdom": "^16.2.5",
    "@types/node": "^14.14.31",
    "@types/pg": "^7.14.4",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^4.0.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1"
  }
}
