{
  "name": "@culthub/yandex-afisha",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Igor Golovin <lislon@mail.ru>",
  "license": "unlicensed",
  "private": true,
  "esm": true,
  "mode": "auto",
  "scripts": {
    "start": "/bin/true",
    "build": "tsc --build && eslint .",
    "test": "jest --passWithNoTests",
    "db:up": "cross-env TZ=Europe/Moscow db-migrate up --table=p_migrations",
    "db:down": "cross-env TZ=Europe/Moscow db-migrate down --table=p_migrations",
    "db:new": "cross-env TZ=Europe/Moscow db-migrate create --table=p_migrations",
    "dev:cron:parse-base": "ts-node --transpile-only -r dotenv/config -r source-map-support/register src/cron/parse-base.ts",
    "dev:cron:parse-diff": "ts-node --transpile-only -r dotenv/config -r source-map-support/register src/cron/parse-diff.ts",
    "dev:cron:try": "ts-node --transpile-only -r dotenv/config -r source-map-support/register src/cron/try.ts",
    "cron:parse-base": "cross-env TZ=Europe/Moscow node dist/cron/parse-base.js",
    "cron:parse-diff": "cross-env TZ=Europe/Moscow node dist/cron/parse-diff.js"
  },
  "dependencies": {
    "@culthub/google-docs": "^1.0.0",
    "@culthub/interfaces": "^1.0.0",
    "@culthub/pg-utils": "^1.0.0",
    "@culthub/universal-db-sync": "^1.0.0",
    "btoa": "^1.2.1",
    "crawler": "^1.3.0",
    "date-fns": "^2.16.1",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "form-data": "3.0.0",
    "glob": "^7.1.6",
    "glob-promise": "^3.4.0",
    "googleapis": "^67.0.0",
    "got": "^11.8.2",
    "jsdom": "^16.4.0",
    "mailgun.js": "^3.3.0",
    "pg": "^8.4.2",
    "pg-promise": "^10.6.1",
    "source-map-support": "^0.5.19",
    "typescript": "^4.1.5",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/btoa": "^1.2.3",
    "@types/crawler": "^1.2.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/jsdom": "^16.2.5",
    "@types/pg": "^7.14.4"
  }
}
