{
  "name": "afisha-parser",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "dotenv ts-node t/parser.ts --cache-directory dist/ts-node-dev-cache --transpile-only",
    "parse:orig": "dotenv ts-node  -r source-map-support/register t/parser.ts -- --orig",
    "parse:snapshot": "dotenv ts-node  -r source-map-support/register t/parser.ts -- --snapshot",
    "convert": "dotenv ts-node t/convert.ts",
    "build": "tsc",
    "dev": "ts-node-dev -r source-map-support/register --respawn --ignore-watch node_modules t/parser.ts --cache-directory dist/ts-node-dev-cache --transpile-only",
    "dev-no-restart": "tsc && nodemon --inspect"
  },
  "dependencies": {
    "@types/node": "^14.14.31",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1"
  }
}
