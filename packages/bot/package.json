{
  "name": "bot",
  "version": "0.0.0",
  "private": true,
  "license": "unlicensed",
  "esm": true,
  "mode": "auto",
  "scripts": {
    "start": "cross-env TZ=Europe/Moscow node dist/start.js",
    "build": "tsc --build && yarn run lint && copyfiles -u 1 'src/**/assets/*.png' 'src/**/*.yaml' dist",
    "dev": "ts-node-dev -r dotenv/config -r source-map-support/register --respawn --ignore-watch node_modules --cache-directory dist/ts-node-dev-cache --transpile-only src/start.ts",
    "dev-no-restart": "tsc && nodemon --inspect",
    "lint": "eslint . --quiet",
    "test": "cross-env TZ=Europe/Moscow jest --runInBand && cross-env TZ=Europe/Moscow cucumber-js",
    "test-cucumber": "cross-env TZ=Europe/Moscow cucumber-js",
    "db:sync": "cross-env TZ=Europe/Moscow node dist/dbsync/run-dbsync.js",
    "db:up": "cross-env TZ=Europe/Moscow db-migrate up",
    "db:down": "cross-env TZ=Europe/Moscow db-migrate down",
    "db:up-test": "cross-env DATABASE_URL=postgres://cult-test:cult-test@localhost:5434/cult-test db-migrate up",
    "db:down-test": "cross-env DATABASE_URL=postgres://cult-test:cult-test@localhost:5434/cult-test db-migrate down",
    "db:new": "cross-env TZ=Europe/Moscow db-migrate create",
    "cron:refresh-events": "cross-env TZ=Europe/Moscow node dist/cron/run-refresh",
    "cron:reset-mailing-counters": "cross-env TZ=Europe/Moscow node dist/cron/reset-mailing-counters",
    "cron:update-latlng": "cross-env TZ=Europe/Moscow node dist/cron/update-latlng"
  },
  "dependencies": {
    "@culthub/interfaces": "^1.0.0",
    "@culthub/google-docs": "^1.0.0",
    "@culthub/pg-utils": "^1.0.0",
    "@culthub/timetable": "^1.0.0",
    "@culthub/universal-db-sync": "^1.0.0",
    "@okta/jwt-verifier": "^2.0.1",
    "@types/debug": "^4.1.5",
    "anymatch": "^3.1.1",
    "bottleneck": "^2.19.5",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.2",
    "date-fns": "^2.16.1",
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^8.2.0",
    "emoji-regex": "^9.2.0",
    "express": "^4.17.1",
    "googleapis": "^68.0.0",
    "got": "^11.8.2",
    "image-type": "^4.1.0",
    "lodash": "^4.17.20",
    "md5": "^2.3.0",
    "node-fetch": "^2.6.1",
    "parsimmon": "^1.16.0",
    "pg": "^8.4.2",
    "pg-monitor": "^1.4.1",
    "pg-promise": "^10.6.1",
    "plural-ru": "^2.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "redis-cli": "^2.0.0",
    "redis-mock": "^0.56.3",
    "slugify": "^1.4.6",
    "source-map-support": "^0.5.19",
    "swagger-jsdoc": "^7.0.0-rc.6",
    "swagger-ui-express": "^4.1.6",
    "telegraf": "4.1.0",
    "telegraf-i18n": "^6.6.0",
    "telegraf-inline-menu": "^6.0.0",
    "telegraf-session-redis": "^5.1.0",
    "telegraf-throttler": "^0.5.1",
    "universal-analytics": "^0.4.23",
    "uuid": "^8.3.1",
    "winston": "^3.3.3",
    "xml-js": "^1.6.11",
    "xstate": "^4.15.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.0.0-rc.0",
    "@types/debug": "^4.1.5",
    "@types/emoji-regex": "^9.2.0",
    "@types/express": "^4.17.8",
    "@types/image-type": "^4.0.1",
    "@types/lodash": "^4.14.161",
    "@types/md5": "^2.2.1",
    "@types/node-fetch": "^2.5.7",
    "@types/parsimmon": "^1.10.4",
    "@types/pg": "^7.14.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/redis-mock": "^0.17.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/universal-analytics": "^0.4.4",
    "@types/uuid": "^8.3.0",
    "@types/yamljs": "^0.2.31",
    "concurrently": "^6.0.0",
    "copyfiles": "^2.4.1",
    "esm": "^3.2.25",
    "njwt": "^1.0.0",
    "nodemon": "^2.0.4",
    "typegram": "3.1.10"
  },
  "engines": {
    "yarn": "1.x"
  }
}
