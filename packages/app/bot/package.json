    "slugify": "^1.4.6",
    "source-map-support": "^0.5.19",
    "swagger-jsdoc": "^7.0.0-rc.6",
    "swagger-ui-express": "^4.1.6",
    "telegraf": "4.3.0",
    "telegraf-i18n": "^6.6.0",
    "telegraf-inline-menu": "^6.2.1",
    "telegraf-session-redis": "^5.1.0",
    "telegraf-throttler": "0.6.0",
    "universal-analytics": "^0.4.23",
    "uuid": "^8.3.1",
