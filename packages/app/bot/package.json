{
  "name": "bot",
  "version": "0.0.0",
  "private": true,
  "license": "unlicensed",
  "esm": true,
  "mode": "auto",
  "scripts": {
    "start": "cross-env TZ=Europe/Moscow node -r source-map-support/register dist/start.js",
    "build": "tsc --project tsconfig.build.json && yarn run lint && copyfiles -u 1 'src/**/assets/*.png' 'src/**/*.yaml' 'src/**/*.html' dist && copyfiles -u 4 '../../lib/web/build/**' 'dist/web-backend/frontend'",
    "dev": "yarn run ts-node-dev --watch 'src/lib/**/*.html,.env' -r dotenv/config -r source-map-support/register --respawn --ignore-watch node_modules,.next --cache-directory dist/ts-node-dev-cache --transpile-only ./src/start.ts",
    "dev-no-restart": "tsc && nodemon --inspect",
    "hh": "ts-node-dev src/web-backend/backend/site-router.ts",
    "lint": "cross-env TZ=Europe/Moscow eslint .",
    "test": "echo 'test' && cross-env TZ=Europe/Moscow jest --runInBand && cross-env TZ=Europe/Moscow cucumber-js",
    "test-cucumber": "cross-env TZ=Europe/Moscow cucumber-js",
    "db:sync": "cross-env TZ=Europe/Moscow node dist/dbsync/run-dbsync.js",
    "db:up": "cross-env TZ=Europe/Moscow db-migrate up",
    "db:down": "cross-env TZ=Europe/Moscow db-migrate down",
    "db:up-test": "cross-env DATABASE_URL=postgres://cult-test:cult-test@localhost:5434/cult-test db-migrate up",
    "db:down-test": "cross-env DATABASE_URL=postgres://cult-test:cult-test@localhost:5434/cult-test db-migrate down",
    "db:new": "cross-env TZ=Europe/Moscow db-migrate create",
    "cron:refresh-events": "cross-env TZ=Europe/Moscow node dist/cron/run-refresh",
    "cron:reset-mailing-counters": "cross-env TZ=Europe/Moscow node dist/cron/reset-mailing-counters",
    "cron:update-latlng": "cross-env TZ=Europe/Moscow node dist/cron/update-latlng",
    "dev:cron:refresh-events": "ts-node --transpile-only -r dotenv/config -r source-map-support/register src/cron/run-refresh.ts"
  },
  "dependencies": {
    "@culthub/google-docs": "workspace:*",
    "@culthub/interfaces": "workspace:*",
    "@culthub/pg-utils": "workspace:*",
    "@culthub/timetable": "workspace:*",
    "@culthub/universal-db-sync": "workspace:*",
    "@culthub/web": "workspace:*",
    "@okta/jwt-verifier": "^2.0.1",
