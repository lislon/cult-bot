    "eslint": "^7.23.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "esm": "^3.2.25",
    "expect": "~26.6.0",
    "jest": "26.6.0",
    "njwt": "^1.0.0",
    "nodemon": "^2.0.4",
    "superagent": "^6.1.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.0.0",
