{
  "name": "@culthub/yandex-afisha",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Igor Golovin <lislon@mail.ru>",
  "license": "unlicensed",
  "private": true,
  "esm": true,
  "mode": "auto",
  "scripts": {
    "start": "/bin/true",
    "build": "tsc --build && eslint .",
    "test": "jest --passWithNoTests",
    "db:up": "cross-env TZ=Europe/Moscow db-migrate up --table=p_migrations",
    "db:down": "cross-env TZ=Europe/Moscow db-migrate down --table=p_migrations",
    "db:new": "cross-env TZ=Europe/Moscow db-migrate create --table=p_migrations",
    "dev:cron:parse-base": "ts-node --transpile-only -r dotenv/config -r source-map-support/register src/cron/parse-base.ts",
    "dev:cron:parse-diff": "ts-node --transpile-only -r dotenv/config -r source-map-support/register src/cron/parse-diff.ts",
    "dev:cron:try": "ts-node --transpile-only -r dotenv/config -r source-map-support/register src/cron/try.ts",
    "cron:parse-base": "cross-env TZ=Europe/Moscow node dist/cron/parse-base.js",
    "cron:parse-diff": "cross-env TZ=Europe/Moscow node dist/cron/parse-diff.js"
  },
  "dependencies": {
    "@culthub/google-docs": "workspace:*",
    "@culthub/interfaces": "workspace:*",
    "@culthub/pg-utils": "workspace:*",
    "@culthub/timetable": "workspace:*",
    "@culthub/universal-db-sync": "workspace:*",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "btoa": "^1.2.1",
    "crawler": "^1.3.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.16.1",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "express": "^4.17.3",
    "form-data": "3.0.0",
    "glob": "^7.1.6",
    "glob-promise": "^3.4.0",
    "googleapis": "^67.0.0",
    "got": "^11.8.2",
    "jest": "26.6.0",
    "jsdom": "^16.4.0",
    "lodash": "^4.17.21",
    "mailgun.js": "^3.3.0",
    "pg": "^8.4.2",
    "pg-promise": "^10.6.1",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.5",
    "winston": "^3.3.3",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@jest/types": "^27.0.2",
    "@types/btoa": "^1.2.3",
    "@types/crawler": "^1.2.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/jsdom": "^16.2.5",
    "@types/lodash": "^4.14.161",
    "@types/pg": "^7.14.4",
    "@types/yargs": "^16.0.0"
  }
}
