{
  "name": "bot",
  "version": "0.0.0",
  "private": true,
  "license": "unlicensed",
  "esm": true,
  "mode": "auto",
  "scripts": {
    "start": "cross-env TZ=Europe/Moscow node dist/start.js",
    "build": "tsc && npm run lint",
    "dev": "ts-node-dev -r source-map-support/register --respawn --ignore-watch node_modules src/start.ts",
    "dev-no-restart": "tsc && nodemon --inspect",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "cross-env TZ=Europe/Moscow jest && cross-env TZ=Europe/Moscow cucumber-js",
    "test-cucumber": "cross-env TZ=Europe/Moscow cucumber-js",
    "db:sync": "cross-env TZ=Europe/Moscow node dist/dbsync/run-dbsync.js",
    "db:up": "cross-env TZ=Europe/Moscow db-migrate up",
    "db:down": "cross-env TZ=Europe/Moscow db-migrate down",
    "db:up-test": "cross-env DATABASE_URL=postgres://cult-test:cult-test@localhost:5434/cult-test db-migrate up",
    "db:down-test": "cross-env DATABASE_URL=postgres://cult-test:cult-test@localhost:5434/cult-test db-migrate down",
    "cron:shuffle-events": "cross-env TZ=Europe/Moscow node dist/cron/run-shuffle"
  },
  "dependencies": {
    "anymatch": "^3.1.1",
    "cross-env": "^7.0.2",
    "date-fns": "^2.16.1",
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "googleapis": "^60.0.1",
    "lodash": "^4.17.20",
    "parsimmon": "^1.16.0",
    "pg": "^8.4.2",
    "pg-monitor": "^1.4.1",
    "pg-promise": "^10.6.1",
    "plural-ru": "^2.0.2",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "source-map-support": "^0.5.19",
    "telegraf": "^3.38.0",
    "telegraf-i18n": "^6.6.0",
    "telegraf-session-redis": "^5.1.0",
    "telegraf-throttler": "^0.2.0",
    "telegraf-update-logger": "^1.2.0",
    "universal-analytics": "^0.4.23",
    "uuid": "^8.3.1",
    "winston": "^3.3.3",
    "xstate": "^4.13.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.0.0-rc.0",
    "@databases/pg-test": "^2.0.0",
    "@types/emoji-regex": "^8.0.0",
    "@types/parsimmon": "^1.10.4",
    "@types/universal-analytics": "^0.4.4",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.14.6",
    "@types/pg": "^7.14.7",
    "@types/request-promise": "^4.1.46",
    "@types/uuid": "^8.3.0",
    "concurrently": "^5.3.0",
    "emoji-regex": "^9.2.0",
    "esm": "^3.2.25",
    "jest": "^26.6.1",
    "jest-serial-runner": "^1.1.0",
    "nodemon": "^2.0.4",
    "telegram-typings": "^4.0.0-beta",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  }
}
