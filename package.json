{
  "name": "bot",
  "version": "0.0.0",
  "private": true,
  "esm": true,
  "mode": "auto",
  "scripts": {
    "start": "node dist/bot-start.js",
    "build": "tsc && npm run lint",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"tsc -w\" \"nodemon -r source-map-support/register --inspect\"",
    "dev-no-restart": "tsc && nodemon --inspect",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest",
    "db:sync": "node dist/dbsync/run-dbsync.js",
    "db:up": "db-migrate up",
    "db:down": "db-migrate down",
    "db:up-test": "cross-env DATABASE_URL=postgres://cult-test:cult-test@localhost:5434/cult-test db-migrate up",
    "db:down-test": "cross-env DATABASE_URL=postgres://cult-test:cult-test@localhost:5434/cult-test db-migrate down"
  },
  "dependencies": {
    "@types/parsimmon": "^1.10.2",
    "anymatch": "^3.1.1",
    "cross-env": "^7.0.2",
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^8.2.0",
    "googleapis": "^60.0.1",
    "knex": "^0.21.5",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "parsimmon": "^1.16.0",
    "pg": "^8.3.2",
    "pg-promise": "^10.6.1",
    "plural-ru": "^2.0.2",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "source-map-support": "^0.5.19",
    "telegraf": "^3.38.0",
    "telegraf-i18n": "^6.6.0",
    "telegraf-session-redis": "^5.1.0",
    "telegraf-throttler": "^0.2.0",
    "telegraf-update-logger": "^1.2.0",
    "winston": "^3.3.3",
    "xstate": "^4.13.0"
  },
  "devDependencies": {
    "@databases/pg-test": "^2.0.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.6.0",
    "@types/pg": "^7.14.4",
    "@types/request-promise": "^4.1.46",
    "concurrently": "^5.3.0",
    "esm": "^3.2.25",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.4.0",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  }
}
